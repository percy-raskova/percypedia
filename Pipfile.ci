sphinx = ">=6.0"
myst-parser = ">=1.0"
furo = "*"
sphinx-design = "*"
sphinx-tags = "*"
sphinx-hoverxref = "*"
jsonschema = ">=4.0"
pyyaml = "*"

[dev-packages]

[requires]
python_version = "3.13"
